<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFFFF"/>
            <corners android:radius="10dp"/>
            <stroke android:width="2dp" android:color="#FF0033A0"/>
            <padding android:left="12dp" android:top="2dp" android:right="12dp" android:bottom="2dp"/>
            <size android:minHeight="16dp" android:minWidth="92dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#FFEAF2FF"/>
            <corners android:radius="10dp"/>
            <stroke android:width="2dp" android:color="#FF0033A0"/>
            <padding android:left="12dp" android:top="2dp" android:right="12dp" android:bottom="2dp"/>
            <size android:minHeight="16dp" android:minWidth="92dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFFFF"/>
            <corners android:radius="10dp"/>
            <stroke android:width="2dp" android:color="#FF0033A0"/>
            <padding android:left="12dp" android:top="2dp" android:right="12dp" android:bottom="2dp"/>
            <size android:minHeight="16dp" android:minWidth="92dp"/>
        </shape>
    </item>
</selector>
